<%#= binding.pry %>

<%= form_for @holiday do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.date_field :start_date, :placeholder => 'dd/mm/yyyy' %>
    <%= f.date_field :end_date, :placeholder => 'dd/mm/yyyy' %>
  </div>
  <%= f.submit "Post", class: "btn btn-large btn-primary" %>
<% end %>